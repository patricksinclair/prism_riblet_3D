FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//libs ("libdynamicRefine2D.so"); //only need this for 2D dynamic mesh refinement

application     interFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

//endTime         15.;
endTime         0.01;

deltaT          0.0001;

writeControl    adjustableRunTime;

writeInterval   0.005;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.95;
maxAlphaCo      0.95;

maxDeltaT       1;



